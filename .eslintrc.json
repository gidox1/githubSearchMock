{
  "env": {
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "extends": "",
  "rules": {
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "comma-dangle": ["error", "never"],
    "no-tabs": 0,
    "no-trailing-spaces": 0,
    "no-param-reassign": 0,
    "arrow-body-style": 0,
    "no-undef": 0,
    "class-methods-use-this": 0,
    "no-useless-escape": 0
  }
}
